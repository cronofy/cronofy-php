{
    "name": "cronofy/cronofy",
    "description": "PHP wrapper for Cronofy's unified calendar API",
    "version": "v1.5.0",
    "require": {
        "php": ">=7.1"
    },
    "autoload": {
        "files": [
            "src/Cronofy.php"
        ],
        "psr-4": {
            "Cronofy\\": "src/"
        }
    },
    "keywords": [
        "cronofy",
        "calendar",
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authentication"
    ],
    "minimum-stability": "beta",
    "license": "MIT",
    "require-dev": {
        "phpunit/phpunit": "^8",
        "squizlabs/php_codesniffer": "3.*"
    }
}
